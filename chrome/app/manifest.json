{
    "manifest_version": 2,
    "name": "Selfnet IPTV App",
    "version": "0.0.1",

    "app": {
        "background": {
            "scripts": ["main.js"]
        }
    },

    "sockets": {
        "udp": {
            "bind": "*",
            "multicastMembership": ""
        }
    },

    "platforms": [
        {
            "nacl_arch": "x86-64",
            "sub_package_path": "_platform_specific/x86-64/"
        }
    ]
}
